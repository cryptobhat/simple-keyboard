<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:showNumberRow="true">
            <!-- U+0C9F: "ಟ" KANNADA LETTER TTA -->
            <Key latin:keySpec="&#x0C9F;" />
            <!-- U+0CA1: "ಡ" KANNADA LETTER DDA -->
            <Key latin:keySpec="&#x0CA1;" />
            <!-- U+0C8E: "ಎ" KANNADA LETTER E
                 U+0C8F: "ಏ" KANNADA LETTER EE (long press) -->
            <Key
                latin:keySpec="&#x0C8E;"
                latin:moreKeys="&#x0C8F;" />
            <!-- U+0CB0: "ರ" KANNADA LETTER RA
                 U+0C8B: "ಋ" KANNADA LETTER VOCALIC R (long press) -->
            <Key
                latin:keySpec="&#x0CB0;"
                latin:moreKeys="&#x0C8B;" />
            <!-- U+0CA4: "ತ" KANNADA LETTER TA -->
            <Key latin:keySpec="&#x0CA4;" />
            <!-- U+0CAF: "ಯ" KANNADA LETTER YA
                 U+0C90: "ಐ" KANNADA LETTER AI (long press) -->
            <Key
                latin:keySpec="&#x0CAF;"
                latin:moreKeys="&#x0C90;" />
            <!-- U+0C89: "ಉ" KANNADA LETTER U -->
            <Key latin:keySpec="&#x0C89;" />
            <!-- U+0C87: "ಇ" KANNADA LETTER I -->
            <Key latin:keySpec="&#x0C87;" />
            <!-- U+0C92: "ಒ" KANNADA LETTER O -->
            <Key latin:keySpec="&#x0C92;" />
            <!-- U+0CAA: "ಪ" KANNADA LETTER PA -->
            <Key latin:keySpec="&#x0CAA;" />
        </case>
        <default>
            <!-- U+0C9F: "ಟ" KANNADA LETTER TTA -->
            <Key latin:keySpec="&#x0C9F;" />
            <!-- U+0CA1: "ಡ" KANNADA LETTER DDA -->
            <Key latin:keySpec="&#x0CA1;" />
            <!-- U+0C8E: "ಎ" KANNADA LETTER E
                 U+0C8F: "ಏ" KANNADA LETTER EE (long press) -->
            <Key
                latin:keySpec="&#x0C8E;"
                latin:moreKeys="&#x0C8F;" />
            <!-- U+0CB0: "ರ" KANNADA LETTER RA
                 U+0C8B: "ಋ" KANNADA LETTER VOCALIC R (long press) -->
            <Key
                latin:keySpec="&#x0CB0;"
                latin:moreKeys="&#x0C8B;" />
            <!-- U+0CA4: "ತ" KANNADA LETTER TA -->
            <Key latin:keySpec="&#x0CA4;" />
            <!-- U+0CAF: "ಯ" KANNADA LETTER YA
                 U+0C90: "ಐ" KANNADA LETTER AI (long press) -->
            <Key
                latin:keySpec="&#x0CAF;"
                latin:moreKeys="&#x0C90;" />
            <!-- U+0C89: "ಉ" KANNADA LETTER U -->
            <Key latin:keySpec="&#x0C89;" />
            <!-- U+0C87: "ಇ" KANNADA LETTER I -->
            <Key latin:keySpec="&#x0C87;" />
            <!-- U+0C92: "ಒ" KANNADA LETTER O -->
            <Key latin:keySpec="&#x0C92;" />
            <!-- U+0CAA: "ಪ" KANNADA LETTER PA -->
            <Key latin:keySpec="&#x0CAA;" />
        </default>
    </switch>
</merge>
