<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:showNumberRow="true">
            <!-- U+0CA0: "ಠ" KANNADA LETTER TTHA -->
            <Key latin:keySpec="&#x0CA0;" />
            <!-- U+0CA2: "ಢ" KANNADA LETTER DDHA -->
            <Key latin:keySpec="&#x0CA2;" />
            <!-- U+0C8F: "ಏ" KANNADA LETTER EE -->
            <Key latin:keySpec="&#x0C8F;" />
            <!-- U+0C8B: "ಋ" KANNADA LETTER VOCALIC R
                 U+0CE0: "ೠ" KANNADA LETTER VOCALIC RR (long press) -->
            <Key
                latin:keySpec="&#x0C8B;"
                latin:moreKeys="&#x0CE0;" />
            <!-- U+0CA5: "ಥ" KANNADA LETTER THA -->
            <Key latin:keySpec="&#x0CA5;" />
            <!-- U+0C90: "ಐ" KANNADA LETTER AI -->
            <Key latin:keySpec="&#x0C90;" />
            <!-- U+0C8A: "ಊ" KANNADA LETTER UU -->
            <Key latin:keySpec="&#x0C8A;" />
            <!-- U+0C88: "ಈ" KANNADA LETTER II -->
            <Key latin:keySpec="&#x0C88;" />
            <!-- U+0C93: "ಓ" KANNADA LETTER OO -->
            <Key latin:keySpec="&#x0C93;" />
            <!-- U+0CAB: "ಫ" KANNADA LETTER PHA -->
            <Key latin:keySpec="&#x0CAB;" />
        </case>
        <default>
            <!-- U+0CA0: "ಠ" KANNADA LETTER TTHA -->
            <Key latin:keySpec="&#x0CA0;" />
            <!-- U+0CA2: "ಢ" KANNADA LETTER DDHA -->
            <Key latin:keySpec="&#x0CA2;" />
            <!-- U+0C8F: "ಏ" KANNADA LETTER EE -->
            <Key latin:keySpec="&#x0C8F;" />
            <!-- U+0C8B: "ಋ" KANNADA LETTER VOCALIC R
                 U+0CE0: "ೠ" KANNADA LETTER VOCALIC RR (long press) -->
            <Key
                latin:keySpec="&#x0C8B;"
                latin:moreKeys="&#x0CE0;" />
            <!-- U+0CA5: "ಥ" KANNADA LETTER THA -->
            <Key latin:keySpec="&#x0CA5;" />
            <!-- U+0C90: "ಐ" KANNADA LETTER AI -->
            <Key latin:keySpec="&#x0C90;" />
            <!-- U+0C8A: "ಊ" KANNADA LETTER UU -->
            <Key latin:keySpec="&#x0C8A;" />
            <!-- U+0C88: "ಈ" KANNADA LETTER II -->
            <Key latin:keySpec="&#x0C88;" />
            <!-- U+0C93: "ಓ" KANNADA LETTER OO -->
            <Key latin:keySpec="&#x0C93;" />
            <!-- U+0CAB: "ಫ" KANNADA LETTER PHA -->
            <Key latin:keySpec="&#x0CAB;" />
        </default>
    </switch>
</merge>
