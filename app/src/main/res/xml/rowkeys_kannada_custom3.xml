<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+0CA3: "ಣ" KANNADA LETTER NNA -->
    <Key latin:keySpec="&#x0CA3;" />
    <!-- U+0CB7: "ಷ" KANNADA LETTER SSA
         U+0CBC: "಼" KANNADA SIGN NUKTA (long press) -->
    <Key
        latin:keySpec="&#x0CB7;"
        latin:moreKeys="&#x0CBC;" />
    <!-- U+0C9A: "ಚ" KANNADA LETTER CA -->
    <Key latin:keySpec="&#x0C9A;" />
    <!-- U+0CB5: "ವ" KANNADA LETTER VA
         U+0C94: "ಔ" KANNADA LETTER AU (long press) -->
    <Key
        latin:keySpec="&#x0CB5;"
        latin:moreKeys="&#x0C94;" />
    <!-- U+0CAC: "ಬ" KANNADA LETTER BA -->
    <Key latin:keySpec="&#x0CAC;" />
    <!-- U+0CA8: "ನ" KANNADA LETTER NA -->
    <Key latin:keySpec="&#x0CA8;" />
    <!-- U+0CAE: "ಮ" KANNADA LETTER MA
         U+0C82: "ಂ" KANNADA SIGN ANUSVARA (long press) -->
    <Key
        latin:keySpec="&#x0CAE;"
        latin:moreKeys="&#x0C82;" />
</merge>
