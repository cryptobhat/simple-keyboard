<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+0C85: "ಅ" KANNADA LETTER A -->
    <Key latin:keySpec="&#x0C85;" />
    <!-- U+0CB8: "ಸ" KANNADA LETTER SA -->
    <Key latin:keySpec="&#x0CB8;" />
    <!-- U+0CA6: "ದ" KANNADA LETTER DA -->
    <Key latin:keySpec="&#x0CA6;" />
    <!-- U+0CCD: "್" KANNADA SIGN VIRAMA (halant) -->
    <Key latin:keySpec="&#x0CCD;" />
    <!-- U+0C97: "ಗ" KANNADA LETTER GA -->
    <Key latin:keySpec="&#x0C97;" />
    <!-- U+0CB9: "ಹ" KANNADA LETTER HA
         U+0C83: "ಃ" KANNADA SIGN VISARGA (long press) -->
    <Key
        latin:keySpec="&#x0CB9;"
        latin:moreKeys="&#x0C83;" />
    <!-- U+0C9C: "ಜ" KANNADA LETTER JA -->
    <Key latin:keySpec="&#x0C9C;" />
    <!-- U+0C95: "ಕ" KANNADA LETTER KA -->
    <Key latin:keySpec="&#x0C95;" />
    <!-- U+0CB2: "ಲ" KANNADA LETTER LA -->
    <Key latin:keySpec="&#x0CB2;" />
</merge>
